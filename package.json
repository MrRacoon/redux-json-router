{
  "name": "redux-json-router",
  "version": "0.0.0",
  "description": "A redux router with simple json routing config",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://redux-json-router.firebaseapp.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/mksarge/redux-json-router.git"
  },
  "bugs": {
    "url": "https://github.com/mksarge/redux-json-router/issues"
  },
  "author": "Michael Sargent <michael-sargent@hotmail.com> (https://mksarge.com)",
  "keywords": [
    "redux",
    "json",
    "router"
  ],
  "engines": {
    "node": ">= 4"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "webpack": "^2.3.2"
  },
  "scripts": {
    "build": "npm run clean && npm run build:node",
    "build:node": "babel src --out-dir lib",
    "clean": "rimraf lib dist",
    "lint": "eslint src test",
    "test": "npm run lint && mocha --compilers js:babel-core/register test/**/*.js"
  }
}
